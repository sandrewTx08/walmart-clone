{
  "name": "walmart-clone",
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon server/index.ts\"",
    "codegen": "graphql-codegen --config codegen.yml",
    "build": "tsc --project tsconfig.node.json && vite build",
    "start": "concurrently \"vite preview\" \"node dist/server/index.js\"",
    "test": "jest"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@prisma/client": "^4.8.0",
    "fastify": "^4.10.2",
    "graphql-request": "^5.1.0",
    "mercurius": "^11.4.0",
    "nexus": "^1.3.0",
    "prisma": "^4.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "concurrently": "^7.6.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.4"
